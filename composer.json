{
    "name": "skore-labs/laravel-json-api",
    "description": "Integrate JSON:API resources on Laravel",
    "license": "MIT",
    "keywords": [
        "skore-labs",
        "laravel-json-api",
        "json-api"
    ],
    "authors": [
        {
            "name": "Ruben Robles",
            "email": "d8vjork@outlook.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/http": "~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0"
    },
    "require-dev": {
        "ext-json": "*",
        "orchestra/testbench": "~3.6.0|~3.7.0|~3.8.0",
        "phpunit/phpunit": "^7.0|^8.0"
    },
    "suggest": {
        "spatie/laravel-query-builder": "Allows to include data on your API responses following the JSON:API",
        "spatie/laravel-json-api-paginate": "Change a little bit the default pagination params of Laravel for the JSON:API ones"
    },
    "autoload": {
        "psr-4": {
            "SkoreLabs\\JsonApi\\": "src"
        }
    }
}
